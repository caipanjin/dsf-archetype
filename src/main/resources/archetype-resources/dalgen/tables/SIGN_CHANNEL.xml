#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<table sqlname="SIGN_CHANNEL">
	<operation name="getSignChannel" paramtype="object"
		multiplicity="many">
		<sql>
			SELECT
			ID,
			INST_ID ,
			INST_NAME ,
			inst_channel ,
			channel_type,
			bank_id,
			Support_CARD_TYPE,
			support_sign_type,
			Status,
			MEMO,
			gmt_create ,
			gmt_modified
			FROM SIGN_CHANNEL
		</sql>
		<sqlmap>
		<![CDATA[
			SELECT 
               ID,
			   INST_ID ,
		       INST_NAME ,
		       inst_channel ,
		       channel_type,
			   bank_id,
		       Support_CARD_TYPE,
		       support_sign_type,
		       Status,
			   MEMO,
		       gmt_create ,
		       gmt_modified
			FROM SIGN_CHANNEL
			where 1 = 1
			<dynamic>
				<isNotNull prepend="and" property="instChannel">
					inst_channel = ${symbol_pound}instChannel${symbol_pound}
				</isNotNull>
				<isNotNull prepend="and" property="channelType">
					channel_type = ${symbol_pound}channelType${symbol_pound}
				</isNotNull>
				<isNotNull prepend="and" property="bankId">
					bank_id = ${symbol_pound}bankId${symbol_pound}
				</isNotNull>
			</dynamic>
			]]>
		</sqlmap>
	</operation>
</table>