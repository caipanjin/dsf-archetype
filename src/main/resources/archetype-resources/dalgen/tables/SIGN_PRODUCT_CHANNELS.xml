#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<table sqlname="SIGN_PRODUCT_CHANNELS">
	<operation name="getSignProductChannels" paramtype="object"
		multiplicity="many">
		<sql>
			SELECT
			ID,
			sign_product_id,
			sign_channel_id,
			operator ,
			Status ,
			MEMO ,
			gmt_create ,
			gmt_modified
			FROM SIGN_PRODUCT_CHANNELS
		</sql>
		<sqlmap>
		<![CDATA[
			SELECT 
               ID,
			   sign_product_id,
			   sign_channel_id,
			   operator ,
		       Status ,
		       MEMO ,
		       gmt_create ,
		       gmt_modified
			FROM SIGN_PRODUCT_CHANNELS
			where 1 = 1
			<dynamic>
				<isNotNull prepend="and" property="signProductId">
					sign_product_id = ${symbol_pound}signProductId${symbol_pound}
				</isNotNull>
				<isNotNull prepend="and" property="signChannelId">
					sign_channel_id = ${symbol_pound}signChannelId${symbol_pound}
				</isNotNull>
				<isNotNull prepend="and" property="status">
					Status = ${symbol_pound}status${symbol_pound}
				</isNotNull>
			</dynamic>
			]]>
		</sqlmap>
	</operation>

	<operation name="insert" paramtype="object">
		<sql>
			INSERT INTO SIGN_PRODUCT_CHANNELS(
			ID,
			sign_product_id,
			sign_channel_id,
			operator ,
			Status ,
			MEMO ,
			gmt_create ,
			gmt_modified)
			VALUES(
			?,
			?,
			?,
			?,
			?,
			?,
			sysdate,
			sysdate
			)
		</sql>
	</operation>

	<operation name="update" paramtype="object">
		<sql>
			UPDATE SIGN_PRODUCT_CHANNELS
			SET
			operator = ? ,
			Status = ? ,
			MEMO = ? ,
			gmt_create = ? ,
			gmt_modified = ?
		</sql>
		<sqlmap>
		<![CDATA[
			UPDATE SIGN_PRODUCT_CHANNELS
			SET 
			<dynamic>
				<isNotNull property="operator">
					operator = ${symbol_pound}operator${symbol_pound},
				</isNotNull>
				<isNotNull property="status">
					Status = ${symbol_pound}status${symbol_pound},
				</isNotNull>
				<isNotNull property="memo">
					MEMO = ${symbol_pound}memo${symbol_pound},
				</isNotNull>
				<isNotNull property="gmtCreate">
					gmt_create = ${symbol_pound}gmtCreate${symbol_pound},
				</isNotNull>
			</dynamic>
			gmt_modified = ${symbol_pound}gmtModified${symbol_pound}
			where 1 = 1
			<dynamic>
				<isNotNull prepend="and" property="signProductId">
					sign_product_id = ${symbol_pound}signProductId${symbol_pound}
				</isNotNull>
				<isNotNull prepend="and" property="signChannelId">
					sign_channel_id = ${symbol_pound}signChannelId${symbol_pound}
				</isNotNull>
			</dynamic>
			]]>
		</sqlmap>
	</operation>
</table>