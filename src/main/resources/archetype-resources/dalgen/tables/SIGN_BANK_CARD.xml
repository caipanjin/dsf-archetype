			INST_NAME = ? ,
			inst_country_code = ?,
			inst_district_code = ?,
			inst_province = ?,
			inst_city = ?,
			inst_branch =
			?,
			user_name = ?,
			certificate_type = ?,
			certificate_no = ?,
			Mobile_phone
			= ?,
			Tel_phone = ?,
			gmt_create = ?,
			gmt_modified = ?
		</sql>
		<sqlmap>
		<![CDATA[
			UPDATE SIGN_BANK_CARD
			SET 
			<dynamic>
				<isNotNull property="cardType">
					CARD_TYPE = ${symbol_pound}cardType${symbol_pound},
				</isNotNull>
				<isNotNull property="instId">
