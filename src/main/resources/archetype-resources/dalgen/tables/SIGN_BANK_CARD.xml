			where 1 = 1
			<dynamic>
				<isNotNull prepend="and" property="id">
					id = ${symbol_pound}id${symbol_pound}
				</isNotNull>
				<isNotNull prepend="and" property="cardNo">
					CARD_NO = ${symbol_pound}cardNo${symbol_pound}
				</isNotNull>
				<isNotNull prepend="and" property="cardType">
