#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<table sqlname="SIGN_BANK_CARD">
	<operation name="getSignBankCard" paramtype="object"
		multiplicity="many">
		<sql>
			select
			ID,
			CARD_NO,
			CARD_TYPE,
			INST_ID ,
			INST_NAME ,
			inst_country_code,
			inst_district_code,
			inst_province,
			inst_city,
			inst_branch,
			user_name,
			certificate_type,
			certificate_no,
			Mobile_phone ,
			Tel_phone ,
			gmt_create ,
			gmt_modified
			from SIGN_BANK_CARD 
		</sql>
		<sqlmap>
		<![CDATA[
			select
			ID,
			CARD_NO,
			CARD_TYPE,
			INST_ID ,
			INST_NAME ,
			inst_country_code ,
			inst_district_code,
			inst_province,
			inst_city,
			inst_branch,
			user_name,
			certificate_type,
			certificate_no,
			Mobile_phone ,
			Tel_phone ,
			gmt_create ,
			gmt_modified
			from SIGN_BANK_CARD
			where 1 = 1
			<dynamic>
				<isNotNull prepend="and" property="id">
					id = ${symbol_pound}id${symbol_pound}
				</isNotNull>
				<isNotNull prepend="and" property="cardNo">
					CARD_NO = ${symbol_pound}cardNo${symbol_pound}
				</isNotNull>
				<isNotNull prepend="and" property="cardType">
					CARD_TYPE = ${symbol_pound}cardType${symbol_pound}
				</isNotNull>
			</dynamic>
			]]>
		</sqlmap>
	</operation>

	<operation name="insert" paramtype="object">
